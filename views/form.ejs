    <section id="contact">
        <form method="POST" action="/post">
        <div class="container">
            
            <table class="table table-striped">
                <thead>
                  <tr>
                    
                    <th scope="col-6">
                        
                            <div class="form-group">
                                <h3>ข้อมูลสัตว์</h3>
                                <label for="form-text" class="col col-form-label">เลขประจำตัว</label>
                                <input class="form-control" name="id">
                                <label for="form-text" class="col col-form-label">สถานะ</label>
                                <input class="form-control" name="status">
                                <label for="form-text" class="col col-form-label">ชื่อสัตว์</label>
                                <input class="form-control" name="_name">
                                <label for="form-text" class="col col-form-label">ชนิด</label>
                                <input class="form-control" name="type">
                                <label for="form-text" class="col col-form-label">พันธุ์</label>
                                <input class="form-control" name="type1">
                                <label for="form-text" class="col col-form-label">เพศ</label>
                                <input class="form-control" name="type2">
                                <label for="form-text" class="col col-form-label">เกิด</label>
                                <input class="form-control" name="day">
                                <label for="form-text" class="col col-form-label">อายุ</label>
                                <input class="form-control" name="aage">
                                <label for="form-text" class="col col-form-label">น้ำหนัก</label>
                                <input class="form-control" name="int">
                            </div>
                          
                    </th>
                    <th scope="col-6">
                        <div class="form-group">
                            <h3>ข้อมูลเจ้าของสัตว์</h3>
                            <label for="form-text" class="col col-form-label">ชื่อเจ้าของสัตว์</label>
                            <input class="form-control" name="pn">
                            <label for="form-text" class="col col-form-label">นามสกุล</label>
                            <input class="form-control" name="ls">
                            <label for="form-text" class="col col-form-label">ที่อยู่</label>
                            <input class="form-control" name="ad">
                            <label for="form-text" class="col col-form-label">แขวง/ตำบล</label>
                            <input class="form-control" name="ad1">
                            <label for="form-text" class="col col-form-label">แขวง/อำเภอ</label>
                            <input class="form-control" name="ad2">
                            <label for="form-text" class="col col-form-label">จังหวัด</label>
                            <input class="form-control" name="ad3">
                            <label for="form-text" class="col col-form-label">รหัสไปรษณีย์</label>
                            <input class="form-control" name="ad4">
                            <label for="form-text" class="col col-form-label">โทรศัพท์</label>
                            <input class="form-control" name="tel">
                            <br>
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                    </th>
